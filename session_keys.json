import json
import os

def load_session_keys():
    path = "session_keys.json"
    if not os.path.exists(path):
        with open(path, "w") as f:
            json.dump({}, f)
        return {}

    with open(path, "r") as f:
        try:
            return json.load(f)
        except json.JSONDecodeError:
            return {}
